<?xml version='1.0' encoding='utf-8'?>
<widget id="com.arqmedia.paintcalc" version="3.3.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Paint Calc</name>
    <description>
        An app to calculate the paint required for decorating.
    </description>
    <author email="info@arqmedia.co.uk" href="https://arqmedia.co.uk/paint-calc/">
        Chris lamb
    </author>
    <content src="index.html" />
    <gap:plugin name="phonegap-admob" source="npm" />
    <config-file parent="GADApplicationIdentifier" target="*-Info.plist">
        <string>ca-app-pub-8816517022745547~1128143402</string>
    </config-file>
    <edit-config file="*-Info.plist" mode="merge" target="UIStatusBarHidden">
        <true />
    </edit-config>
    <edit-config file="*-Info.plist" mode="merge" target="UIViewControllerBasedStatusBarAppearance">
        <false />
    </edit-config>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="phonegap-version" value="cli-8.0.0" />
</widget>
